{
  "debug": true,
  "local": {
    "filepath": "localdb"
  },
  "dynamodb": {
    "table_name": "MyDynamoDBTable"
  },
  "etcd": {
    "host": "localhost",
    "port": 2379,
    "username": "myuser",
    "password": "mypassword",
    "use_ssl": true,
    "cert_file": "path/to/client-cert.pem",
    "key_file": "path/to/client-key.pem",
    "ca_cert_file": "path/to/ca-cert.pem",
    "timeout": 5
  },
  "queue_batch": {
    "size": 10,
    "timeout": 2.0
  }
}